# ================================
# Environment Variables Setup
# ================================
# 
# This is an example file. To use it:
# 1. Copy this file to apps/web/.env
#    cp .env.example apps/web/.env
# 2. Update the values with your actual credentials
#
# ================================

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/dbname
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
ENCRYPTION_KEY=your-64-character-hex-encryption-key-here-32bytes

# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Optional: Sentry
SENTRY_ORGANIZATION=
SENTRY_PROJECT=
NEXT_PUBLIC_SENTRY_DSN=
NEXT_PUBLIC_SENTRY_DISABLED=

# Optional: PostHog Analytics
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=

# Optional: Better Stack
NEXT_PUBLIC_BETTER_STACK_SOURCE_TOKEN=
NEXT_PUBLIC_BETTER_STACK_INGESTING_HOST=

# Optional: Vercel
VERCEL_ENV=
VERCEL_URL=
VERCEL_PROJECT_PRODUCTION_URL=
VERCEL_BYPASS_TOKEN=

# Optional: Arcjet
ARCJET_KEY=

# Optional: Build
ANALYZE=false

# Optional: CI/CD
CI=
CHECKLY_EMAIL_ADDRESS=
CHECKLY_PROJECT_NAME=
CHECKLY_LOGICAL_ID=
ENVIRONMENT_URL=

# Optional: App
NEXT_PUBLIC_APP_URL=http://localhost:3000
